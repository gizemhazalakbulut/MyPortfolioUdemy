@model List<About>
@{
    ViewData["Title"] = "AboutList";
    Layout = "~/Views/Layout/Index.cshtml";
}

<div class="content">
	<div class="container-fluid">
		<h4 class="page-title">Hakkımda</h4>
		<div class="row">
			<div class="col-md-12">
				<div class="card">
					<div class="card-body">
						<table class="table table-typo">
							<tbody>
							
									<tr>
										<td>
											<p>Hakkımda</p>

										</td>
									<td><span class="h6">@ViewBag.v1</span></td>
									<td><a href="/About/UpdateAbout/@ViewBag.v2" class="btn btn-success">Güncelle</a></td>
									</tr>
								
							</tbody>
						</table>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>

